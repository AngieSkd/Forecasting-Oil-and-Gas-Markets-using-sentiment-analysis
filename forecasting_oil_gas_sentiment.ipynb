   "source": [
    "Q1 = ts_df['LOG_RETURN'].quantile(0.25)\n",
    "Q3 = ts_df['LOG_RETURN'].quantile(0.75)\n",
    "IQR = Q3 - Q1\n",
    "lower = Q1 - 1.5 * IQR\n",
    "upper = Q3 + 1.5 * IQR\n",
    "\n",
    "ts_df['outlier_iqr'] = (ts_df['LOG_RETURN'] < lower) | (ts_df['LOG_RETURN'] > upper)"
   ]
